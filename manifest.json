{
  "manifest_version": 2,
  "name": "Mushroomify",
  "description": "Chrome extension to change all webpictures into fungi's picture",
  "author": "Arnaud MARIE",
  "content_scripts": [
    {
    "matches": ["<all_urls>"],
     "js": ["scripts/mushroomify.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Mushroomify"
  },
  "version": "1",
  "permissions": ["tabs"],
  "icons": {
    "16": "images/icons/icon16.png",
    "32": "images/icons/icon32.png",
    "48": "images/icons/icon48.png",
    "128": "images/icons/icon72.png"
  }
}